#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int N;
    cin >>N;
    
    vector<int> numbers(N);
    
    for(int i=0;i<N;i++){
        cin >> numbers[i];
    }
    int minValue = numbers[0];
    int maxValue = numbers[0];
    
     for (int i = 1; i < N; i++) {
        if (numbers[i] < minValue) {
            minValue = numbers[i];
        }
        if (numbers[i] > maxValue) {
            maxValue = numbers[i];
        }
    }

    cout << minValue << ' ' << maxValue;
    return 0;
}
