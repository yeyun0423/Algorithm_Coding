import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        String S = input.nextLine().trim();
        
        if (S.length() == 0) {
            System.out.println(0);
        } else {
            String[] words = S.split("\\s+");
            System.out.println(words.length);
        }
    }
}
